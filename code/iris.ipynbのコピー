    "import seaborn as sns\n",
    "iris_df = sns.load_dataset('iris') # データセットの読み込み\n",
    "iris_df = iris_df[(iris_df['species']=='versicolor') | (iris_df['species']=='virginica')] # 簡単のため、2品種に絞る\n",
    "iris_df.head()"
   ]
